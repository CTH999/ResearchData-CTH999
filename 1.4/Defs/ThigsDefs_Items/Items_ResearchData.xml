<?xml version="1.0" encoding="utf-8"?>
<Defs>

  <!--=============== Research Papers ====================-->
   <ThingDef ParentName="ResourceBase">
    <defName>ResearchPapers</defName>
    <label>Research Papers</label>
    <description>Research Papers are used for simple research and is written by examining one of the 3 basic research samples.\n\nResearch papers last 24 hours when doing simple research.</description>
    <graphicData>
      <texPath>ResearchPapers</texPath>
      <graphicClass>Graphic_StackCount</graphicClass>
    </graphicData>
    <soundInteract>Standard_Drop</soundInteract>
    <soundDrop>Standard_Drop</soundDrop>
    <techLevel>Neolithic</techLevel>
    <thingSetMakerTags>
      <li>RewardStandardMidFreq</li>
    </thingSetMakerTags>
    <statBases>
      <MaxHitPoints>60</MaxHitPoints>
      <MarketValue>18</MarketValue>
      <Mass>0.024</Mass>
      <Flammability>2.0</Flammability>
      <DeteriorationRate>6</DeteriorationRate>
    </statBases>
    <burnableByRecipe>true</burnableByRecipe>
    <healthAffectsPrice>false</healthAffectsPrice>
    <thingCategories>
      <li>Research</li>
    </thingCategories>
  </ThingDef>
  
  <!--=============== Research Data ====================-->
   <ThingDef ParentName="ResourceBase">
