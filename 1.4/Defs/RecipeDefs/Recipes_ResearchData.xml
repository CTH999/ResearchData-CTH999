    <products>
      <AnalyzerData>1</AnalyzerData>
    </products>
	<recipeUsers>
		<li>ResearchSampleBench</li>
	</recipeUsers>
	<workSkill>Intellectual</workSkill>
	<skillRequirements>
      <Intellectual>8</Intellectual>
    </skillRequirements>
  </RecipeDef> 

	<!-- Extract - Archotech Data -->
  <RecipeDef>
    <defName>ArchotechDataRecipe</defName>
    <label>Extract Archotech Data</label>
    <description>Archotech data can only be found, rewarded, or extracted from rare artifacts. Archotech data are required for the archotech analyzer to function when doing research.\n\nArchotech data last 3 days when being researched at the archotech analyzer.</description>
    <jobString>Extracting Archotech Data.</jobString>
    <workAmount>4000</workAmount>
    <workSpeedStat>GeneralLaborSpeed</workSpeedStat>
    <effectWorking>Cook</effectWorking>
    <soundWorking>Recipe_Smith</soundWorking>
    <fixedIngredientFilter>
      <categories>
        <li>Artifacts</li>
      </categories>
	  <thingDefs>
		<li>Apparel_PsychicShockLance</li>
		<li>Apparel_PsychicInsanityLance</li>
		<li>AIPersonaCore</li>
	  </thingDefs>
    </fixedIngredientFilter>
    <ingredients>
      <li>
        <filter>
          <categories>
            <li>Artifacts</li>
          </categories>
		  <thingDefs>
			<li>Apparel_PsychicShockLance</li>
			<li>Apparel_PsychicInsanityLance</li>
			<li>AIPersonaCore</li>
		  </thingDefs>
        </filter>
        <count>1</count>
      </li>
    </ingredients>
	<researchPrerequisites>
      <li>MultiAnalyzer</li>
    </researchPrerequisites>
    <products>
      <ArchotechData>1</ArchotechData>
    </products>
	<recipeUsers>
		<li>ResearchSampleBench</li>
	</recipeUsers>
	<workSkill>Intellectual</workSkill>
	<skillRequirements>
	  <Crafting>6</Crafting>
      <Intellectual>10</Intellectual>
    </skillRequirements>
  </RecipeDef> 

</Defs>
