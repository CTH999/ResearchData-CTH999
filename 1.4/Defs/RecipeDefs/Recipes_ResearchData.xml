<?xml version="1.0" encoding="UTF-8"?>
<Defs>

	<!-- Write - Research Papers -->
  <RecipeDef>
    <defName>ResearchPapersRecipe</defName>
    <label>Write Research Papers</label>
    <description>Research Papers are used for simple research and is written by examining one of the 3 basic research samples.\n\nResearch papers last 24 hours when doing simple research.</description>
    <jobString>Writing Research Papers.</jobString>
    <workAmount>600</workAmount>
    <workSpeedStat>GeneralLaborSpeed</workSpeedStat>
    <effectWorking>Tailor</effectWorking>
    <soundWorking>Interact_Research</soundWorking>
	<ingredients>
      <li>
        <filter>
			<thingDefs>
				<li>OrganicResearchSamples</li>
				<li>MetallicResearchSamples</li>
				<li>FabricResearchSamples</li>
			</thingDefs>
        </filter>
        <count>1</count>
      </li>
    </ingredients>
	<fixedIngredientFilter>
			<thingDefs>
				<li>OrganicResearchSamples</li>
				<li>MetallicResearchSamples</li>
				<li>FabricResearchSamples</li>
			</thingDefs>
    </fixedIngredientFilter>
    <products>
      <ResearchPapers>1</ResearchPapers>
    </products>
	<recipeUsers>
		<li>ResearchSampleBench</li>
	</recipeUsers>
	<workSkill>Intellectual</workSkill>
	<skillRequirements>
      <Intellectual>1</Intellectual>
    </skillRequirements>
  </RecipeDef> 
  
	<!-- Assemble - Research Data -->
  <RecipeDef>
    <defName>ResearchDataRecipe</defName>
    <label>Assemble Research Data</label>
    <description>Research Data are used for high-tech research and is Assemble with steel, silver and all 3 basic research samples.\n\nResearch data last 24 hours when doing high-tech research.</description>
    <jobString>Assembling Research Data.</jobString>
    <workAmount>800</workAmount>
    <workSpeedStat>GeneralLaborSpeed</workSpeedStat>
    <effectWorking>Cook</effectWorking>
    <soundWorking>Recipe_Smith</soundWorking>
    <ingredients>
      <li>
        <filter>
          <thingDefs>
            <li>OrganicResearchSamples</li>
          </thingDefs>
        </filter>
        <count>1</count>
      </li>
      <li>
        <filter>
          <thingDefs>
            <li>MetallicResearchSamples</li>
          </thingDefs>
        </filter>
        <count>1</count>
      </li>
      <li>
        <filter>
          <thingDefs>
            <li>FabricResearchSamples</li>
          </thingDefs>
        </filter>
        <count>1</count>
      </li>
      <li>
        <filter>
          <thingDefs>
            <li>Steel</li>
          </thingDefs>
        </filter>
        <count>10</count>
      </li>
      <li>
        <filter>
          <thingDefs>
            <li>Silver</li>
          </thingDefs>
        </filter>
        <count>2</count>
      </li>
    </ingredients>
    <fixedIngredientFilter>
      <thingDefs>
        <li>OrganicResearchSamples</li>
        <li>MetallicResearchSamples</li>
        <li>FabricResearchSamples</li>
        <li>Steel</li>
        <li>Silver</li>
      </thingDefs>
    </fixedIngredientFilter>
	<researchPrerequisites>
      <li>MicroelectronicsBasics</li>
    </researchPrerequisites>
    <products>
      <ResearchData>1</ResearchData>
    </products>
	<recipeUsers>
		<li>ResearchSampleBench</li>
	</recipeUsers>
	<workSkill>Intellectual</workSkill>
	<skillRequirements>
      <Intellectual>5</Intellectual>
    </skillRequirements>
  </RecipeDef> 
  
	<!-- Assemble - Analyzer Data -->
  <RecipeDef>
    <defName>AnalyzerDataRecipe</defName>
    <label>Assemble Analyzer Data</label>
    <description>Analyzer Data are required for the multi-analyzer to function. Its expansive, and use gold and all 4 types og research samples.\n\nAnalyzer Data last 3 days when being researched at the multi-analyzer.</description>
    <jobString>Assembling Analyzer Data.</jobString>
    <workAmount>1200</workAmount>
    <workSpeedStat>GeneralLaborSpeed</workSpeedStat>
    <effectWorking>Cook</effectWorking>
    <soundWorking>Recipe_Smith</soundWorking>
    <ingredients>
      <li>
        <filter>
          <thingDefs>
            <li>OrganicResearchSamples</li>
          </thingDefs>
        </filter>
        <count>1</count>
      </li>
      <li>
        <filter>
          <thingDefs>
            <li>MetallicResearchSamples</li>
          </thingDefs>
        </filter>
        <count>1</count>
      </li>
      <li>
        <filter>
          <thingDefs>
            <li>FabricResearchSamples</li>
          </thingDefs>
        </filter>
        <count>1</count>
      </li>
      <li>
        <filter>
          <thingDefs>
            <li>ComponentResearchSamples</li>
          </thingDefs>
        </filter>
        <count>1</count>
      </li>
      <li>
        <filter>
          <thingDefs>
            <li>Steel</li>
          </thingDefs>
        </filter>
        <count>10</count>
      </li>
      <li>
        <filter>
          <thingDefs>
            <li>Gold</li>
          </thingDefs>
        </filter>
